{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Talking Rock",
  "identifier": "dev.reos.app",
  "version": "0.0.0",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Talking Rock",
        "width": 1920,
        "height": 1080
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["main-capability"]
    }
  }
}
